{
  "ReverseProxy": {
    "Routes": {
      "complainthubRoute": {
        "ClusterId": "complainthubCluster",
        "Match": {
          "Path": "/complainthub/{**catch-all}"
        }
      },
      "lockoutHubRoute": {
        "ClusterId": "complainthubCluster",
        "Match": {
          "Path": "/lockoutHub/{**catch-all}"
        }
      },
      "notificationHubRoute": {
        "ClusterId": "complainthubCluster",
        "Match": {
          "Path": "/notificationHub/{**catch-all}"
        }
      },
      "defaultRoute": {
        "ClusterId": "backendCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "complainthubCluster": {
        "Destinations": {
          "API-Instance-1": { "Address": "http://localhost:5001/" }
        }
      },
      "backendCluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "API-Instance-1": { "Address": "http://localhost:5001/" },
          "API-Instance-2": { "Address": "http://localhost:5002/" }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}